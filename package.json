{
  "name": "issue-tracker",
  "version": "1.0.0",
  "description": "issue tracker MERN Stack",
  "engines": {
    "node": ">=13.0.0",
    "npm": ">=6.0.0"
  },
  "main": "server.js",
  "private": true,
  "scripts": {
    "start": "react-scripts build",
    "poststart": "env NODE_ENV=production node server.js",
    "devClient": "react-scripts start",
    "devServer": "env NODE_ENV=development nodemon --ignore './src' server.js ",
    "eject": "react-scripts eject"
  },
  "author": "Ignacio Bejarano",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "clsx": "^1.0.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "material-table": "^1.69.2",
    "mongoose": "^5.7.8",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "typeface-roboto": "^0.0.75"
  },
  "devDependencies": {
    "nodemon": "^2.0.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
